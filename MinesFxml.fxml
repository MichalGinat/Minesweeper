<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mines.ControllerMines">
   <children>
      <VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="115.0">
         <children>
            <Button fx:id="resetButton" alignment="CENTER" contentDisplay="TOP" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Reset">
               <VBox.margin>
                  <Insets right="10.0" top="70.0" />
               </VBox.margin>
            </Button>
            <HBox alignment="CENTER_RIGHT" prefWidth="200.0">
               <children>
                  <Label alignment="TOP_RIGHT" text="width" />
                  <TextField fx:id="width" prefHeight="20.0" prefWidth="50.0" text="10">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets right="10.0" top="7.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER_RIGHT" text="height">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></Label>
                  <TextField fx:id="height" prefHeight="20.0" prefWidth="50.0" text="10">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets right="10.0" top="7.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER_RIGHT" text="mines" />
                  <TextField fx:id="mines" prefHeight="20.0" prefWidth="50.0" text="10">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <padding>
                  <Insets right="10.0" />
               </padding>
               <VBox.margin>
                  <Insets top="7.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
      <StackPane fx:id="gridID" prefHeight="300.0" prefWidth="300.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></StackPane>
   </children>
</HBox>
